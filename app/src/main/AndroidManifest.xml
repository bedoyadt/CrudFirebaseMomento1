<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crudfirebasemomento1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EliminarActivity"
            android:label="@string/title_activity_eliminar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".EditarActivity"
            android:label="@string/title_activity_editar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetalleActivity"
            android:label="@string/title_activity_detalle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NuevoActivity"
            android:label="@string/title_activity_nuevo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>